game.StarterGui:SetCore("SendNotification",  {
 Title = "Randomizer Infinite Ammo ";
 Text = "Respawning for load the script pls wait...";
 Icon = "";
 Duration = 10;
 Callback = NotificationBindable;
})

game.Players.LocalPlayer.Character.Humanoid.Health = 0

wait(5)
game.StarterGui:SetCore("SendNotification",  {
 Title = "Script Loaded âœ…";
 Text = "Enjoy !";
 Icon = "";
 Duration = 5;
 Callback = NotificationBindable;
})

local freddy = math.huge
local glen = freddy


local mt = getrawmetatable(game);
make_writeable(mt);
local old_index = mt.__index;

mt.__index = function(alfred,gilbert)
   if tostring(alfred) == "Mag" then
   if tostring(gilbert) == "Value" then
       return glen;
   end
   end
   return old_index(alfred,gilbert);
end

local freddy = math.huge
local glen = freddy


local mt = getrawmetatable(game);
make_writeable(mt);
local old_index = mt.__index;

mt.__index = function(alfred,gilbert)
   if tostring(alfred) == "Ammo" then
   if tostring(gilbert) == "Value" then
       return glen;
   end
   end
   return old_index(alfred,gilbert);
   end
